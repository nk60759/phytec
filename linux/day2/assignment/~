#include<stdio.h>
#include<math.h>

int main()
{
	float x1,x2,y1,y2;
	float res;
	printf("Input x1:");
	scanf("%f \n",&x1);
	printf("Input y1:");
        scanf("%f \n",&y1);
	printf("Input x2:");
        scanf("%f \n",&x2);
	printf("Input y2:");
        scanf("%f \n",&y2);	
	res =((x2-x1)*(x2-x1))+((y2-y1)*(y2-y1));
	printf("Distance between the said points:%.4f \n",sqrt(res));
	return 0;
}
